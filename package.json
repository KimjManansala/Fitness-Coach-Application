{
  "name": "fitness-coach-application",
  "version": "1.0.0",
  "description": "This project was inspired to help keep trainers connected, check in on progress, and maintain payments with their clients. The learning goal of this project is to work with the following new technologies 1. React-Native 2. MongoDB 3. Payment APIs 4. Work on a windows enviroment",
  "main": "index.js",
  "scripts": {
    "server": "nodemon ./Server/Server.js",
    "start": "node ./Server/Server.js",
    "start_react": "cd client && npm run start",
    "development": "concurrently \"npm run server\" \"npm run start_react\"",
    "lint:check": "eslint . --ext=js,jsx;  exit 0",
    "lint:fix": "eslint . --ext=js,jsx --fix;  exit 0",
    "test": "echo \"Error: no test specified\" && exit 1",
    "db:create:test": "cross-env NODE_ENV=test npx sequelize-cli db:create"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KimjManansala/Fitness-Coach-Application.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/KimjManansala/Fitness-Coach-Application/issues"
  },
  "homepage": "https://github.com/KimjManansala/Fitness-Coach-Application#readme",
  "dependencies": {
    "axios": "^0.21.1",
    "compression": "^1.7.4",
    "concurrently": "^6.1.0",
    "dotenv": "^8.2.0",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "history": "^4.7.2",
    "nodemon": "^1.18.3",
    "passport": "^0.4.1",
    "passport-google-oauth20": "^2.0.0",
    "path": "^0.12.7",
    "perfect-scrollbar": "1.5.0",
    "pg": "^8.5.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^4.44.4",
    "sequelize-views-support": "^1.2.2",
    "watch": "^0.13.0"
  },
  "devDependencies": {
    "sequelize-cli": "^5.4.0"
  }
}
