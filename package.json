{
  "name": "fitness-coach-application",
  "version": "1.0.0",
  "description": "This project was inspired to help keep trainers connected, check in on progress, and maintain payments with their clients. The learning goal of this project is to work with the following new technologies 1. React-Native 2. MongoDB 3. Payment APIs 4. Work on a windows enviroment",
  "main": "index.js",
  "scripts": {
    "server": "nodemon ./Server/Server.js",
    "start": "node ./Server/Server.js",
    "build": "webpack --watch --mode=development",
    "prod": "webpack --config webpack.prod.js",
    "lint:check": "eslint . --ext=js,jsx;  exit 0",
    "lint:fix": "eslint . --ext=js,jsx --fix;  exit 0",
    "test": "echo \"Error: no test specified\" && exit 1",
    "app-start": "react-scripts start",
    "app-build": "react-scripts build && gulp licenses",
    "app-test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
    "build-package-css": "node-sass src/assets/scss/material-kit-react.scss dist/material-kit-react.css",
    "build-package": "npm run build-package-css && babel src --out-dir dist",
    "compile-sass": "node-sass src/assets/scss/material-kit-react.scss src/assets/css/material-kit-react.css",
    "minify-sass": "node-sass src/assets/scss/material-kit-react.scss src/assets/css/material-kit-react.min.css --output-style compressed",
    "map-sass": "node-sass src/assets/scss/material-kit-react.scss src/assets/css/material-kit-react.css --source-map true"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KimjManansala/Fitness-Coach-Application.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/KimjManansala/Fitness-Coach-Application/issues"
  },
  "homepage": "https://github.com/KimjManansala/Fitness-Coach-Application#readme",
  "dependencies": {
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-es2015": "^7.0.0-beta.53",
    "@babel/preset-react": "^7.9.4",
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "4.9.1",
    "babel-core": "^7.0.0-0",
    "babel-loader": "8.1.0",
    "body-parser": "^1.18.2",
    "chartist": "0.10.1",
    "classnames": "2.2.6",
    "compression": "^1.7.2",
    "dotenv": "^8.2.0",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "history": "^4.7.2",
    "material-kit-react": "^1.9.0",
    "migrate-mongoose": "^4.0.0",
    "mongoose": "^5.11.17",
    "nodemon": "^1.18.3",
    "path": "^0.12.7",
    "perfect-scrollbar": "1.5.0",
    "prop-types": "^15.6.2",
    "react": "^16.3.2",
    "react-chartist": "0.14.3",
    "react-dom": "^16.3.2",
    "react-google-maps": "9.4.5",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^4.0.8",
    "react-scripts": "^3.4.1",
    "react-swipeable-views": "0.13.9",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "webpack": "4.42.0",
    "webpack-merge": "^4.1.2"
  },
  "devDependencies": {
    "@babel/cli": "7.10.1",
    "@babel/plugin-proposal-class-properties": "7.10.1",
    "@babel/preset-env": "7.10.1",
    "@babel/preset-react": "7.10.1",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-react": "7.20.0",
    "gulp": "4.0.2",
    "gulp-append-prepend": "1.0.8",
    "prettier": "2.0.5",
    "webpack-cli": "^4.5.0"
  }
}
